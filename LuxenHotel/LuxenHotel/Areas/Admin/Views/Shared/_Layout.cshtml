<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>
        @(ViewData["Title"] != null ? $"{ViewData["Title"]} - " : "")Dashboard
    </title>

    @Html.Favicon("/favicon.ico")

    @Html.CdnCss("https://fonts.googleapis.com/css?family=Inter:300,400,500,600,700")
    @Html.CdnCss("https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.0/css/all.min.css")

    @Html.LibStyle("keenthemes/css/plugins.bundle.css")
    @Html.LibStyle("keenthemes/css/style.bundle.css")

    @await RenderSectionAsync("Styles", required: false)
</head>

<body id="kt_app_body" data-kt-app-header-fixed="true" data-kt-app-header-fixed-mobile="true"
    data-kt-app-sidebar-enabled="true" data-kt-app-sidebar-fixed="true" data-kt-app-sidebar-hoverable="true"
    data-kt-app-sidebar-push-toolbar="true" data-kt-app-sidebar-push-footer="true" data-kt-app-toolbar-enabled="true"
    data-kt-app-aside-enabled="true" data-kt-app-aside-fixed="true" data-kt-app-aside-push-toolbar="true"
    data-kt-app-aside-push-footer="true" class="app-default">

    <div class="d-flex flex-column flex-root app-root" id="kt_app_root">
        <!--begin::Page-->
        <div class="app-page  flex-column flex-column-fluid " id="kt_app_page">
            <!--begin::Header-->
            @await Html.PartialAsyncFromArea("Admin", "Partials/_Header")
            <!--end::Header-->
            <div class="app-wrapper flex-column flex-row-fluid" id="kt_app_wrapper">
                <!--begin::Sidebar-->
                @await Html.PartialAsyncFromArea("Admin", "Partials/_Sidebar")
                <!--end::Sidebar-->
                <!--begin::Main-->
                <div class="app-main flex-column flex-row-fluid" id="kt_app_main">
                    <div class="d-flex flex-column flex-column-fluid">
                        <!--begin::Content-->
                        <div id="kt_app_content" class="app-content flex-column-fluid">
                            <div id="kt_app_content_container" class="app-container container-fluid">
                                @RenderBody()
                            </div>
                        </div>
                        <!--end::Content-->
                    </div>
                    @await Html.PartialAsyncFromArea("Admin", "Partials/_Footer")
                </div>
                <!--end:::Main-->
                <!--layout-partial:layout/partials/_aside.html-->
            </div>
        </div>
    </div>

    @Html.LibScript("keenthemes/js/plugins.bundle.js")
    @Html.LibScript("keenthemes/js/scripts.bundle.js")

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>